---
layout: index
---

<article role="article">
	<header>
		<h1><a class="article-permalink" href="{{page.url}}">{{ page.title }}</a></h1>
		<time class="article-pubdate" datetime="{{ page.date | date: '%Y-%m-%d' }}">{{ page.date | date: "%B %e, %Y" }}</time>
	</header>

	{{ content }}

</article>

<footer class="article-footer">
	<nav>
		<dl>
			{% if page.previous.url %}
			<dt class="article-nav-label">Previous article:</dt><dd class="article-nav-link"><a href={{ page.previous.url }} rel="prev">{{ page.previous.title }}</a></dd>
			{% endif %}

			{% if page.next.url %}
			<dt class="article-nav-label">Next article:</dt><dd class="article-nav-link"><a href={{ page.next.url }} rel="next"> {{ page.next.title }}</a></dd>
			{% endif %}
		</dl>
	</nav>

   <div id="disqus_thread"></div>
   <script type="text/javascript">
       var disqus_shortname = 'kevinsuttle';

       (function() {
           var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
           dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
           (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
       })();
   </script>
   <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
   <a href="http://disqus.com" class="dsq-brlink">comments powered by <span class="logo-disqus">Disqus</span></a>

</footer>
